[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
  close_term = <Shift><Alt>q
  close_window = <Primary><Shift><Alt>c
  edit_terminal_title = <Primary><Shift><Alt>f
  edit_window_title = <Primary><Shift><Alt>ISO_Level3_Shift
  go_next = <Alt>eacute
  go_prev = <Alt>ampersand
  new_window = <Primary><Shift>w
  resize_down = <Shift><Alt>Down
  resize_left = <Shift><Alt>Left
  resize_right = <Shift><Alt>Right
  resize_up = <Shift><Alt>Up
  split_horiz = <Shift><Alt>w
  split_vert = <Shift><Alt>x
  toggle_zoom = <Shift><Alt>e
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
